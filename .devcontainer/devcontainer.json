{
    "name": "Django + PostgreSQL",
    "dockerComposeFile": [
      "docker-compose.yml"
    ],
    "service": "web",
    "workspaceFolder": "/workspace",
    "settings": {
      "terminal.integrated.defaultProfile.linux": "bash"
    },
    "extensions": [
      "ms-python.python",
      "ms-python.vscode-pylance"
    ],
    "postCreateCommand": "pip install -r requirements.txt",
    "remoteUser": "vscode"
  }